{
   "name": "typeormcar",
   "version": "0.0.0",
   "private": true,
   "scripts": {
      "start": "ts-node app.ts",
      "start:dev": "nodemon",
      "start2": "ts-node src/index.ts",
      "build": "tsc",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
      "test": "jest --detectOpenHandles --forceExit",
      "test:watch": "jest --watch"
   },
   "dependencies": {
      "@types/jest": "^26.0.23",
      "@types/node": "^15.0.3",
      "@types/react": "^17.0.5",
      "@types/react-dom": "^17.0.5",
      "bcryptjs": "^2.4.3",
      "cookie-parser": "~1.4.4",
      "cors": "^2.8.5",
      "debug": "~2.6.9",
      "express": "~4.16.1",
      "http-errors": "~1.6.3",
      "jade": "~1.11.0",
      "morgan": "~1.9.1",
      "mysql": "^2.14.1",
      "nodemailer": "^6.5.0",
      "pg": "^8.5.1",
      "reflect-metadata": "^0.1.10",
      "rootpath": "^0.1.2",
      "ts-jest": "^26.5.4",
      "ts-node": "^9.1.1",
      "typeorm": "0.2.31",
      "typescript": "^4.2.4"
   },
   "devDependencies": {
      "jest": "^26.6.3",
      "nodemon": "^2.0.7",
      "supertest": "^6.1.3"
   },
   "jest": {
      "moduleFileExtensions": [
         "ts",
         "tsx",
         "js"
      ],
      "transform": {
         "^.+\\.(ts|tsx)$": "ts-jest"
      },
      "globals": {
         "ts-jest": {
            "tsConfigFile": "tsconfig.json"
         }
      },
      "testMatch": [
         "**/tests/*.+(ts|tsx|js)"
      ]
   }
}
